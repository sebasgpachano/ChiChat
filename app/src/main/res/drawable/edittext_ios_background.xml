<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#f3f2f8" />
            <corners android:radius="12dp" />
            <padding
                android:left="8dp"
                android:top="8dp"
                android:right="8dp"
                android:bottom="8dp" />
            <stroke
                android:width="1dp"
                android:color="#CCCCCC" />
        </shape>
    </item>
</layer-list>